{
  "defaultSeverity": "warn",
  "extends": ["tslint:recommended"],
  "rules": {
    "quotemark": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "semicolon": true,
    "trailing-comma":false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
